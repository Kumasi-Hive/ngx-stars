{
    "extends": "../../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "lib",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "lib",
            "kebab-case"
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ]
    }
}
